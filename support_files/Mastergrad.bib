@article{levFugledeConjectureConvex2022,
  title = {The {{Fuglede}} Conjecture for Convex Domains Is True in All Dimensions},
  author = {Lev, Nir and Matolcsi, M{\'a}t{\'e}},
  year = {2022},
  journal = {Acta Mathematica},
  volume = {228},
  number = {2},
  eprint = {1904.12262},
  primaryclass = {math},
  pages = {385--420},
  issn = {00015962, 18712509},
  doi = {10.4310/ACTA.2022.v228.n2.a3},
  url = {http://arxiv.org/abs/1904.12262},
  urldate = {2023-05-21},
  abstract = {A set \$\textbackslash Omega \textbackslash subset \textbackslash mathbb\{R\}\^d\$ is said to be spectral if the space \$L\^2(\textbackslash Omega)\$ has an orthogonal basis of exponential functions. A conjecture due to Fuglede (1974) stated that \$\textbackslash Omega\$ is a spectral set if and only if it can tile the space by translations. While this conjecture was disproved for general sets, it has long been known that for a convex body \$\textbackslash Omega \textbackslash subset \textbackslash mathbb\{R\}\^d\$ the "tiling implies spectral" part of the conjecture is in fact true. To the contrary, the "spectral implies tiling" direction of the conjecture for convex bodies was proved only in \$\textbackslash mathbb\{R\}\^2\$, and also in \$\textbackslash mathbb\{R\}\^3\$ under the a priori assumption that \$\textbackslash Omega\$ is a convex polytope. In higher dimensions, this direction of the conjecture remained completely open (even in the case when \$\textbackslash Omega\$ is a polytope) and could not be treated using the previously developed techniques. In this paper we fully settle Fuglede's conjecture for convex bodies affirmatively in all dimensions, i.e. we prove that if a convex body \$\textbackslash Omega \textbackslash subset \textbackslash mathbb\{R\}\^d\$ is a spectral set then \$\textbackslash Omega\$ is a convex polytope which can tile the space by translations. To prove this we introduce a new technique, involving a construction from crystallographic diffraction theory, which allows us to establish a geometric "weak tiling" condition necessary for a set \$\textbackslash Omega \textbackslash subset \textbackslash mathbb\{R\}\^d\$ to be spectral.},
  archiveprefix = {arxiv},
  keywords = {{42B10, 52B11, 52C07, 52C22},Mathematics - Classical Analysis and ODEs,Mathematics - Functional Analysis,Mathematics - Metric Geometry,notion},
  file = {/Users/max/Zotero/storage/V3QKLWIX/Lev and Matolcsi - 2022 - The Fuglede conjecture for convex domains is true .pdf;/Users/max/Zotero/storage/MDB6HEWT/1904.html}
}

